{
    "name": "node_project_practice",
    "version": "1.0.0",
    "description": "Learning node concepts",
    "main": "./microservices/server.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node server.js"
    },
    "keywords": [
        "Express",
        "REST",
        "API",
        "MongoDB"
    ],
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ]
        }
    },
    "author": "Poonam Gawali",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "eslint-config-airbnb": "^18.0.1",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "faker": "^4.1.0",
        "googleapis": "^45.0.0",
        "i": "^0.3.6",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.7.12",
        "oauth2orize": "^1.11.0",
        "passport": "^0.4.0",
        "passport-http": "^0.3.0",
        "passport-http-bearer": "^1.0.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^5.0.1",
        "sinon": "^7.5.0",
        "swagger-ui-express": "^4.1.2",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "chai": "^3.2.0",
        "chai-http": "^1.0.0",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2"
    }
}
